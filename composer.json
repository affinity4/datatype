{
    "name": "affinity4/datatype",
    "description": "Datatype manipulation library. String, Array and Objects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Watts",
            "email": "luke@affinity4.ie"
        }
    ],
    "autoload": {
        "psr-4": {
            "Affinity4\\Datatype\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "scripts": {
        "test:arr": "phpunit --filter ArrTest tests/ArrTest.php",
        "test:inflector": "phpunit --filter InflectorTest tests/InflectorTest.php",
        "test:num": "phpunit --filter NumTest tests/NumTest.php",
        "test:str": "phpunit --filter StrTest tests/StrTest.php"
    }
}
